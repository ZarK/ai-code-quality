[run]
source = .
omit =
    */tests/*
    */test_*
    */.venv/*
    */venv/*
    */node_modules/*
    */quality/*
    test-projects/*
    test-aiq/*
    test-pure-shell/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
