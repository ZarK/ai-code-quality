{
  "name": "@tjalve/aiq",
  "version": "0.1.5",
  "private": false,
  "type": "module",
  "description": "Universal quality runner (staged pipeline) as an ephemeral npx CLI.",
  "bin": {
    "aiq": "bin/aiq.js",
    "quality": "bin/aiq.js"
  },
  "scripts": {
    "dev": "node bin/quality.js --help",
    "clean": "rm -rf assets/quality && mkdir -p assets/quality",
    "build": "npm run clean && node ./scripts/build-assets.cjs",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "files": [
    "bin",
    "assets/quality",
    "README.md",
    "THIRD_PARTY_NOTICES.md"
  ]
}
